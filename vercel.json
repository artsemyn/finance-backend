{
    "version": 2,
    "crons": [
        {
            "path": "/internal/auto-savings/run",
            "schedule": "0 0 * * *"
        }
    ],
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ]
}
